{
    "name": "questionmuppet/wp-dev-tools",
    "description": "Command-line tools to assist with WordPress plugin and theme development.",
    "homepage": "https://github.com/questionmuppet/wp-dev-tools",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Jason Schousboe",
            "email": "jason.schousboe@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "ulrichsg/getopt-php": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "9.*",
        "dg/bypass-finals": "1.2.1"
    },
    "autoload": {
        "psr-4": {
            "Wp_Dev_Tools\\": "src/"
